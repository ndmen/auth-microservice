{
  "name": "auth-microservice",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "proto:generate": "protoc --plugin=node_modules/.bin/protoc-gen-ts_proto -I=./src/_proto --ts_proto_out=src/_proto src/_proto/**/*.proto --ts_proto_opt=nestJs=true --ts_proto_opt=fileSuffix=.pb --ts_proto_opt=addGrpcMetadata=true --ts_proto_opt=useOptionals=all --ts_proto_opt=unrecognizedEnum=false",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "docker:up": "docker-compose up -d --build",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "typeorm": "env-cmd -f .env.development ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- --dataSource=src/database/data-source.ts migration:generate",
    "migration:create": "npm run typeorm -- migration:create",
    "migration:run": "npm run typeorm -- --dataSource=src/database/data-source.ts migration:run",
    "migration:revert": "npm run typeorm -- --dataSource=src/database/data-source.ts migration:revert",
    "schema:drop": "npm run typeorm -- --dataSource=src/database/data-source.ts schema:drop",
    "seed:run": "ts-node -r tsconfig-paths/register ./src/database/seeds/run-seed.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --verbose"
  },
  "dependencies": {
    "@fastify/compress": "^6.4.0",
    "@fastify/helmet": "^11.0.0",
    "@fastify/multipart": "^7.7.0",
    "@fastify/static": "^6.10.2",
    "@grpc/grpc-js": "^1.8.2",
    "@grpc/proto-loader": "^0.7.4",
    "@nestjs-modules/ioredis": "^1.0.1",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/cache-manager": "^2.0.1",
    "@nestjs/common": "^10.0.5",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.5",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/microservices": "^10.0.5",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-fastify": "^10.0.5",
    "@nestjs/platform-socket.io": "^10.0.5",
    "@nestjs/swagger": "^7.1.1",
    "@nestjs/throttler": "^4.1.0",
    "@nestjs/typeorm": "^10.0.1",
    "@nestjs/websockets": "^10.0.5",
    "@types/supertest": "^2.0.11",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "cache-manager": "^5.1.4",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "fastify": "^4.19.2",
    "form-data": "^4.0.0",
    "google-protobuf": "^3.21.2",
    "ioredis": "^5.3.2",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "nest-access-control": "^2.2.0",
    "nestjs-pino": "^3.1.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "pino": "^8.8.0",
    "pino-http": "^8.3.1",
    "pino-pretty": "^9.1.1",
    "qs": "^6.11.0",
    "redis": "^4.5.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.0",
    "socket.io": "^4.7.1",
    "supertest": "^6.3.3",
    "typeorm": "^0.3.17",
    "typeorm-naming-strategies": "^4.1.0"
  },
  "devDependencies": {
    "@darraghor/eslint-plugin-nestjs-typed": "^3.18.0",
    "@nestjs/cli": "^10.1.7",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.5",
    "@types/cache-manager": "^4.0.2",
    "@types/jest": "29.2.5",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.18",
    "@types/passport-local": "^1.0.36",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "env-cmd": "^10.1.0",
    "eslint": "^8.31.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unicorn": "^47.0.0",
    "jest": "29.3.1",
    "prettier": "^2.8.2",
    "source-map-support": "^0.5.20",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.0.0",
    "ts-proto": "^1.138.0",
    "tsconfig-paths": "4.1.2",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=16"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
